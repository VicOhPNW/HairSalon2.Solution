@{
  Layout = "_Layout";
}

<h3>Here are all the clients for this stylist:</h3>

<ol>
  @foreach (var client in @Model["clients"])
  {
    <li><a href='/stylists/@Model["stylist"].GetId()/client/@client.GetId()'>@client.GetDescription()</a></li>
  }
</ol>

<p><a href='/stylists'>Return to stylists list</a></p>
<p><a href='/'>Return to Main Page</a></p>
